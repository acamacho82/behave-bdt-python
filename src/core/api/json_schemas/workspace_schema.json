{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/workspace.json",
  "type": "object",
  "title": "The Workspace Schema",
  "required": [
    "id",
    "kind",
    "name",
    "person_id",
    "project_ids"
  ],
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The Id Schema",
      "default": 0,
      "examples": [
        7894213
      ]
    },
    "kind": {
      "$id": "#/properties/kind",
      "type": "string",
      "title": "The Kind Schema",
      "default": "",
      "examples": [
        "workspace"
      ],
      "pattern": "^(.*)$"
    },
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The Name Schema",
      "default": "",
      "examples": [
        "api workspace"
      ],
      "pattern": "^(.*)$"
    },
    "person_id": {
      "$id": "#/properties/person_id",
      "type": "integer",
      "title": "The PersonId Schema",
      "default": 0,
      "examples": [
        846512
      ]
    },
    "project_ids": {
      "$id": "#/properties/project_ids",
      "type": "array",
      "title": "The ProjectIds Schema",
      "default": [],
      "examples": [
        [12345,67890]
      ]
    }
  }
}